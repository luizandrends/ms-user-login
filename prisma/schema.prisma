generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Users {
  id               String   @id @default(auto()) @map("_id") @db.ObjectId
  email            String   @unique
  password         String
  userRegisterDbId String   @unique
  createdAt        DateTime @default(now())
  updatedAt        DateTime @updatedAt
}
